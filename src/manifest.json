 {
   "name": "Test Extension",
   "description": "First google chrome extension using Angular",
   "version": "1.0",
   "background":
   {
     "scripts": [
       "background.js",
       "runtime.js"
     ]
   },
   "content_scripts": [
   {
     "js": ["content.js", "runtime.js"],
     "matches": ["*://*.google.com/*"]
   }],
   "browser_action":
   {
     "default_popup": "index.html"
   },
   "manifest_version": 2
 }
